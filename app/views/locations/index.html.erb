<%- model_class = Location -%>
<% [:notice, :error, :alert].each do |level| %>
  <% unless flash[level].blank? %>
    <div class="alert-error">
      <a class="close" href="/locations">Ã—</a>
      <%= content_tag :p, flash[level] %>
    </div>
  <% end %>
<% end %>

<div class="content" style="width:100%">

  <div class="form_ind"><%= form_for :dump, :url => {:controller => "locations",
                                                     :action => "excel"}, :method => "POST", :html => {:multipart => true}, :class => "form_index" do |f| %>
      Select an Excel File :
      <%= f.file_field :excel_file %>
      <%= submit_tag 'Upload', class: 'btn btn-primary index_button' %>
    <% end %>

    <%= link_to t('.Add manually', :default => t("helpers.links.Add manually")),
                new_location_path,
                :class => 'btn btn-primary  ' %></div>
  </br>

  <!--<div id = "locations"><%#= render 'locations'%></div>-->
  <table id="list"></table>
  <div id="pager"></div>

</div>